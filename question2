#include <iostream>
using namespace std;
int main() 
{
    int m,n;
    cin>>m>>n;
    if(m<=0||n<=0||m>10||n>10){
        cout<<"Invalid Input"<<endl;
        return 0;
    }
    double sum=0,val;
    for(int i=0;i<m*n;++i){
        cin>>val;
        sum+=val;
    }
    cout << sum << endl;
    return 0;
}
