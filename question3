#include <iostream>
using namespace std;
int main() 
{
    int a;
    cin>>a;
    if(a<1||a>100){
        cout<<"Invalid Input"<<endl;
        return 0;
    }
    int matrix[100][100];
    int sum=0;
    for(int i=0;i<a;++i){
        for(int j=0;j<a;++j){
            cin>>matrix[i][j];
            if(i==j){
                sum+=matrix[i][j]; 
            }
        }
    }
    cout<<sum<<endl;
    return 0;
}
